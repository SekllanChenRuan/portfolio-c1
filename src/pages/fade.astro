---
import Layout from '../layouts/Layout.astro'
---

<Layout title="Fade">
   <main class="main container">
        <section class="mt-5 pt-5">
          <h2 class="text-center">Fade and Slide Stuff</h2>
        </section>
        <section class="item left">
          <div class="item__content">
            <h2>O_O</h2>
            <p>Never gonna give you up</p>
          </div>
        </section>
        <section class="item right">
          <div class="item__content">
            <h2>=_=</h2>
            <p>Never gonna let you down</p>
          </div>
        </section>
        <section class="item left">
          <div class="item__content">
            <h2>>_O</h2>
            <p>Never gonna run around and desert you</p>
          </div>
        </section>
      </main>

  
  <script is:inline slot="bs-script" src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
  <script is:inline slot="script" src="src/scripts/index.js"></script>
</Layout>


